{
    "version": "1.0.2",
    "summary": "A simple elm wrapper over the HTML5 Media API",
    "repository": "https://github.com/danabrams/elm-media.git",
    "license": "BSD3",
    "source-directories": [
        ".",
        "src",
        "examples"
    ],
    "exposed-modules": [
        "Media",
        "Media.Events",
        "Media.State",
        "Media.Source"
    ],
    "native-modules": true,
    "dependencies": {
        "NoRedInk/elm-decode-pipeline": "3.0.0 <= v < 4.0.0",
        "elm-community/html-extra": "2.2.0 <= v < 3.0.0",
        "elm-community/json-extra": "2.7.0 <= v < 3.0.0",
        "elm-lang/core": "5.0.0 <= v < 6.0.0",
        "elm-lang/html": "2.0.0 <= v < 3.0.0",
        "elm-lang/http": "1.0.0 <= v < 2.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
